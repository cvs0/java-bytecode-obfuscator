{
  "renameClasses": false,
  "renameFields": true,
  "renameMethods": false,
  "namingMode": "SINGLE_CHAR",
  "verbose": false,
  "keepRules": {
    "keepStandardEntryPoints": true,
    "keepClassPatterns": [
      "com/mylib/api/.*",
      "com/mylib/public/.*"
    ],
    "keepMethodPatterns": {
      ".*": [".*public.*"]
    },
    "keepAllMethods": [
      "com/mylib/PublicInterface",
      "com/mylib/api/ApiContract"
    ],
    "keepFieldPatterns": {
      "com/mylib/api/.*": [".*"],
      "com/mylib/constants/.*": [".*"]
    }
  }
}